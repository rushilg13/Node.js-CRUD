<table class="table table-striped">
<thead>
    <tr>
        <th>Full Name</th>
        <th>Email</th>
        <th>Mobile</th>
        <th>City</th>
        <th></th>
    </tr>
</thead>
<tbody>
    {{#each list}}
    <tr>
        <td>{{ this.fullName }}</td>
        <td>{{ this.email }}</td>
        <td>{{ this.mobile }}</td>
        <td>{{ this.city }}</td>
        <td>
            <a><i class="fa fa-pencil fa-lg" aria-hidden="true"></i></a>
            <a><i class="fa fa-trash fa-lg" aria-hidden="true"></i></a>
        </td>
    </tr>
    {{/each}}
</tbody>
</table>